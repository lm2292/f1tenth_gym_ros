         <cylinder length="0.060" radius="0.023"/>
        </geometry>
      </collision>

    <inertial>
      <origin xyz="0 -0.115 0.046" rpy="-1.5708 0 0"/>
      <mass value="0.3"/>
      <inertia
        ixx="0.00013" ixy="0.0" ixz="0.0"
        iyy="0.00013" iyz="0.0"
        izz="0.00008"/>
