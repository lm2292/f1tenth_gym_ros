     <child link="front_wheel"/>
     <origin xyz="0.13 0 0.02" rpy="0 0 0"/>
   </joint>
   
   <link name="scanner">
     <visual>
       <geometry>
         <box size="0.015 0.045 0.015"/>
       </geometry>
       <origin rpy="0 0 0" xyz="0.022 0 0.015"/>
       <material name="black">
         <color rgba="0 0 0 1"/>
       </material>
     </visual>

     <collision>
