<robot name="test_robot">    

  <!-- <link name="world"/> -->
   
   <link name="base_link">     
    <visual>
      <geometry>
          <box size="0.16 0.16 0.12"/> 
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0.085"/>       
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>

    <collision>
        <origin rpy="0 0 0" xyz="0 0 0.085"/>       
        <geometry>
          <box size="0.16 0.16 0.12"/> 
        </geometry>
      </collision>

    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0.085"/>       
      <mass value="0.5"/>
      <inertia
        ixx="0.000256" ixy="0.0" ixz="0.0"
        iyy="0.000256" iyz="0.0"
        izz="0.000512"/>
    </inertial>

   </link>


  <gazebo reference="base_link">
    <mu1>0.2</mu1>
    <mu2>0.2</mu2>
    <material>Gazebo/Black</material>
  </gazebo>


  <link name="left_wheel">
     <visual>
       <geometry>
         <cylinder length="0.008" radius="0.046"/>
       </geometry>
       <origin rpy="0 0 0" xyz="0 0 0"/>
       <material name="black">
         <color rgba="0 0 0 1"/>
