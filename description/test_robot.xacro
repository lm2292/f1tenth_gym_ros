     <child link="right_wheel"/>
     <origin xyz="0 -0.115 0.046" rpy="-1.5708 0 0"/>
   </joint>
   
   <link name="front_wheel_attach">
     <visual>
       <geometry>
         <cylinder length="0.060" radius="0.023"/>
       </geometry>
       <origin rpy="0 0 0" xyz="0.13 0 0.08"/>
       <material name="white">
         <color rgba="1 1 1 1"/>
       </material>
     </visual>

     <collision>
       <origin xyz="0 -0.115 0.046" rpy="-1.5708 0 0"/>
