  </gazebo>

  <link name="imu_link">
     <visual>
       <geometry>
         <box size="0.005 0.005 0.005"/>
       </geometry>
       <origin rpy="0 0 0" xyz="0.022 0 0.01725"/>
       <material name="black">
         <color rgba="0 0 0 1"/>
       </material>
     </visual>

     <collision>
       <origin rpy="0 0 0" xyz="0.022 0 0.01725"/>
        <geometry>
         <box size="0.005 0.005 0.005"/>
        </geometry>
      </collision>

    <inertial>
       <origin rpy="0 0 0" xyz="0.022 0 0.01725"/>
      <mass value="0.05"/>
      <inertia
        ixx="0.000005" ixy="0.0" ixz="0.0"
        iyy="0.0000006" iyz="0.0"
        izz="0.0000056"/>
    </inertial>
   </link>

  <gazebo reference="imu_link">
    <sensor name="imu_sensor" type="imu">
      <always_on>1</always_on>
      <update_rate>1</update_rate>
      <visualize>true</visualize>
      <topic>/imu</topic>
    </sensor>
  </gazebo>
  
   
   <joint name="base_to_scanner_joint" type="fixed">
      <parent link="base_link"/>
      <child link="scanner"/>
      <origin xyz="0.105 0 0.22" rpy="0 0 0"/>
    </joint>

    <joint name="base_to_imu_joint" type="fixed">
      <parent link="base_link"/>
      <child link="imu_link"/>
      <origin xyz="0.105 0 0.2425" rpy="0 0 0"/>
    </joint>

